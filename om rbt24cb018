package omm;

import java.util.Scanner;

public class primee {

	public static void main(String[] args) {
		int n=0,count=0;
		Scanner sc = new Scanner (System.in);
		
		System.out.println("Enter a number: ");
		n = sc.nextInt();
		
		for(int i=2 ; i<=n ; i++)
		{
			if(n%i == 0)
			{
				count = count + 1;
			}
		
		}
		if( count == 1)
		{
			System.out.println(n+" is a prime number");
		}
		else
		{
			System.out.println(n+" is not prime");
		}
	}

}
